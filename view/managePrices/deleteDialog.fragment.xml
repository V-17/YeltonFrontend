<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Покупка" afterClose="pricesDeleteDialog.cancel" initialFocus="buttonCancel">
        <content>
            <Label text='Удалить покупку?' textAlign="Center" width="100%"/>
        </content>
        <beginButton>
            <Button text="Удалить" type="Reject" press="pricesDeleteDialog.apply"/>
        </beginButton>
        <endButton>
            <Button id="buttonCancel" text="Отмена" press="pricesDeleteDialog.cancel"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
