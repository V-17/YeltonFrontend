<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="{i18n>price}" afterClose="pricesDeleteDialog.cancel" initialFocus="buttonCancel">
        <content>
            <Label text="{i18n>deletePriceWarning}" textAlign="Center" width="100%"/>
        </content>
        <beginButton>
            <Button text="{i18n>delete}" type="Reject" press="pricesDeleteDialog.apply"/>
        </beginButton>
        <endButton>
            <Button id="buttonCancel" text="Отмена" press="pricesDeleteDialog.cancel"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
